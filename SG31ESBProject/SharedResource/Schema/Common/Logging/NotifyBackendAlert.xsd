<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	 xmlns="http://www.tibco.com/schemas/SG31ESBProject/SharedResource/Schema/Common/Logging/NotifyBackendAlert.xsd"
	 targetNamespace="http://www.tibco.com/schemas/SG31ESBProject/SharedResource/Schema/Common/Logging/NotifyBackendAlert.xsd"
	 elementFormDefault="qualified"
	 attributeFormDefault="unqualified">
	<xs:element name="NotifyAlert">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MsgId" type="xs:string"/>
				<xs:element name="ServiceName" type="xs:string" minOccurs="0"/>
				<xs:element name="OperationName" type="xs:string" minOccurs="0"/>
				<xs:element name="ErrorDescription" type="xs:string" minOccurs="0"/>
				<xs:element name="CreatedTimeError" type="xs:string" minOccurs="0"/>
				<xs:element name="ErrorSystem" type="xs:string" minOccurs="0"/>
				<xs:element name="FromSystem" type="xs:string" minOccurs="0"/>
				<xs:element name="Severity" type="xs:string" minOccurs="0"/>
				<xs:element name="SubjectContent" type="xs:string" minOccurs="0"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>