        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>contains($_error/ns:ErrorReport/Msg, '@ID:1:1=TRANSACTION IS REVERRED')</pd:xpath>
