                            </xsl:when>
                            <xsl:otherwise>
                                <xsl:attribute name="xsi:nil">true</xsl:attribute>
                            </xsl:otherwise>
                        </xsl:choose>
                    </FILE_SIZE>
                    <SOURCE_SYSTEM>
                        <xsl:value-of select="&quot;SWIFT&quot;"/>
                    </SOURCE_SYSTEM>
                    <RECEIVE_DATE>
                        <xsl:value-of select="current-dateTime()"/>
                    </RECEIVE_DATE>
                    <STATUS>
                        <xsl:value-of select="&quot;&quot;"/>
                    </STATUS>
                </jdbcUpdateActivityInput>
            </pd:inputBindings>
        </pd:activity>
