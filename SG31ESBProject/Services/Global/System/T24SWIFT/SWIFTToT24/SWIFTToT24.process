                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>SOURCE_SYSTEM</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>RECEIVE_DATE</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                    <parameter>
                        <parameterName>STATUS</parameterName>
                        <dataType>VARCHAR</dataType>
                    </parameter>
                </Prepared_Param_DataType>
            </config>
            <pd:inputBindings>
                <jdbcUpdateActivityInput>
                    <FILE_NAME>
                        <xsl:choose>
                            <xsl:when test="exists($ESBServiceState/pfx:ESBServiceState/pfx:filename)">
                                <xsl:value-of select="$ESBServiceState/pfx:ESBServiceState/pfx:filename"/>
                            </xsl:when>
                            <xsl:otherwise>
