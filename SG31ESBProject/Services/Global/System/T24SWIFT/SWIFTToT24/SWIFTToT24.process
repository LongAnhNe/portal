                <jdbcQueryActivityInput>
                    <FILE_NAME>
                        <xsl:choose>
                            <xsl:when test="exists($ESBServiceState/pfx:ESBServiceState/pfx:filename)">
                                <xsl:value-of select="$ESBServiceState/pfx:ESBServiceState/pfx:filename"/>
                            </xsl:when>
