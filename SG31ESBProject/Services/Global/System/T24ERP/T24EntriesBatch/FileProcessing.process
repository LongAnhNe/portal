                <textContent>
                    <xsl:value-of select="concat(&quot;&#xA;&quot;,&quot;Error Message: &quot;,$_error/ns1:ErrorReport/Msg)"/>
                </textContent>
            </pfx:WriteActivityInputTextClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="CallT24ImportProcess">
