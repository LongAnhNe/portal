            <Response>
                <xsl:value-of select="concat(&quot;[FINISH]-Total Record Received: &quot;,$CallT24EntriesBatchFile/Output/TotalRecordsReceived, &quot; - Total Record Processed: &quot;,$CallT24EntriesBatchFile/Output/TotalRecordsProcessed, &quot; - Total Record DB: &quot;,$CountRecordsProcessed/resultSet/Record[1]/CNT )"/>
            </Response>
        </pd:inputBindings>
    </pd:activity>
