        <pd:xpath>$_globalVariables/ns:GlobalVariables/ServiceGlobalVar/T24ERP/DestSystem='ERP' and $CallT24EntriesBatchFile/Output/TotalRecordsReceived = $CountRecordsProcessed/resultSet/Record[1]/CNT</pd:xpath>
    </pd:transition>
