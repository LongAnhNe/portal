    <pd:targetNamespace>http://xmlns.example.com/1447686612439</pd:targetNamespace>
    <pd:activity name="CallT24EntriesBatchFile">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>235</pd:x>
        <pd:y>119</pd:y>
        <config>
            <processName>/Services/Global/System/T24ERP/T24EntriesBatch/T24EntriesBatchRecursive.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <FileName>
                    <xsl:value-of select="$Start/Input/FileName"/>
                </FileName>
                <NumberRecords>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/T24WS/BatchCount"/>
                </NumberRecords>
                <State>
                    <xsl:value-of select="'Online'"/>
                </State>
                <DestSystem>
                    <xsl:value-of select="$_globalVariables/ns:GlobalVariables/ServiceGlobalVar/T24ERP/DestSystem"/>
                </DestSystem>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SetFinish">
        <pd:type>com.tibco.plugin.file.FileWriteActivity</pd:type>
        <pd:resourceType>ae.activities.FileWriteActivity</pd:resourceType>
        <pd:x>577</pd:x>
        <pd:y>286</pd:y>
        <config>
            <encoding>text</encoding>
            <compressFile>None</compressFile>
            <append>true</append>
        </config>
        <pd:inputBindings>
            <pfx:WriteActivityInputTextClass>
                <fileName>
                    <xsl:value-of select="concat(tib:substring-before-last($Start/Input/FileName, '.'), '_ERPResult.txt')"/>
                </fileName>
                <textContent>
                    <xsl:value-of select="$Reponse/Response"/>
                </textContent>
            </pfx:WriteActivityInputTextClass>
        </pd:inputBindings>
