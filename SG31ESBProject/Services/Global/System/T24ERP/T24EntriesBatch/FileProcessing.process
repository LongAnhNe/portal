    <pd:activity name="CallERPProcBatch">
        <pd:type>com.tibco.plugin.jdbc.JDBCCallActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCCallActivity</pd:resourceType>
        <pd:x>766</pd:x>
        <pd:y>358</pd:y>
        <config>
            <timeout>0</timeout>
            <maxRows>100</maxRows>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/SharedResource/Connection/Jdbc/ERPDB.sharedjdbc</jdbcSharedConfig>
            <schema>APPS</schema>
            <catalog>FSAH_CREATE_EVENTS_PKG_T24</catalog>
            <ProcedureName>MAIN_BATCH</ProcedureName>
            <parameterTypes>
                <parameter>
                    <colName>P_DATE</colName>
                    <colType>1</colType>
                    <dataType>12</dataType>
                    <typeName>VARCHAR2</typeName>
                </parameter>
                <parameter>
                    <colName>P_STATUS</colName>
                    <colType>4</colType>
                    <dataType>2</dataType>
                    <typeName>NUMBER</typeName>
                </parameter>
                <parameter>
                    <colName>P_MESSAGE</colName>
                    <colType>4</colType>
                    <dataType>12</dataType>
                    <typeName>VARCHAR2</typeName>
                </parameter>
            </parameterTypes>
        </config>
        <pd:inputBindings>
            <inputs>
                <inputSet>
                    <P_DATE>
                        <xsl:value-of select="string(current-date())"/>
                    </P_DATE>
                </inputSet>
            </inputs>
        </pd:inputBindings>
    </pd:activity>
