                        </xsl:otherwise>
                    </xsl:choose>
                </CONVERSION_RATE>
                <MODE_FLAG>
                    <xsl:value-of select="1"/>
                </MODE_FLAG>
                <STATUS>
                    <xsl:value-of select="&quot;ACTIVE&quot;"/>
                </STATUS>
