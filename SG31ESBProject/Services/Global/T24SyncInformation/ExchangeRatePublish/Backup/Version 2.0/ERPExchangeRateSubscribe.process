            <statement>Insert into EPR_T24_GL_DRATES(FROM_CURRENCY,TO_CURRENCY,FROM_CONVERSION_DATE,TO_CONVERSION_DATE,USER_CONVERSION_TYPE,CONVERSION_RATE,MODE_FLAG,STATUS)
values(?,?,?,?,?,?,?,?)
