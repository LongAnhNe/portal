            <jdbcUpdateActivityInput>
                <FROM_CURRENCY>
                    <xsl:choose>
                        <xsl:when test="exists($Parse-XML/pfx:ExchangeRateFlow/pfx:id)">
                            <xsl:value-of select="$Parse-XML/pfx:ExchangeRateFlow/pfx:id"/>
                        </xsl:when>
                        <xsl:otherwise>
