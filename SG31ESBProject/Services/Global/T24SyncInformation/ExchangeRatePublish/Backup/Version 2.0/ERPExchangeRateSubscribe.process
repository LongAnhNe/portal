                </FROM_CONVERSION_DATE>
                <TO_CONVERSION_DATE>
                    <xsl:value-of select="$Parse-XML/pfx:ExchangeRateFlow/pfx:eventCommon/ns3:today"/>
                </TO_CONVERSION_DATE>
                <USER_CONVERSION_TYPE>
                    <xsl:value-of select="&quot;midRevalRate&quot;"/>
